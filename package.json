{
  "name": "node-raft-redis",
  "version": "0.0.2",
  "description": "",
  "type": "module",
  "main": "./lib/cjs/index.js",
  "typings": "./lib/cjs/index.d.ts",
  "exports": {
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js"
  },
  "scripts": {
    "start": "ts-node-dev --transpile-only src/index.ts",
    "build": "npm run build:esm && npm run  build:cjs",
    "build:esm": "tsc --outDir lib/esm",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs"
  },
  "keywords": [
    "node",
    "raft",
    "redis",
    "consensus"
  ],
  "author": {
    "email": "nitedani@gmail.com",
    "name": "Horváth Dániel"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^17.0.29",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "cuid": "^2.1.8",
    "redis": "^4.0.6"
  },
  "files": [
    "./lib"
  ]
}
